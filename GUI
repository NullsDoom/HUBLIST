		end
	})
 
	addmode("q", {
 
		idle = function()
		t.setWalkSpeed(60)
 
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-0.9-0.1*sin(sine*1),-0.6)*angles(0.6981317007977318-0.08726646259971647*sin(sine*1),-1.4835298641951802-0.08726646259971647*sin(sine*1),0),deltaTime) 
		swordweld.C0=swordweld.C0:Lerp(cf(3.3,1.2,-1.5)*angles(-0.8726646259971648,1.5707963267948966,4.014257279586958),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,-0.2+0.1*sin(sine*1),0)*angles(-2.0943951023931953+0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.7-0.1*sin(sine*1),0.2)*angles(-0.17453292519943295-0.08726646259971647*sin(sine*1),1.4835298641951802+0.08726646259971647*sin(sine*1),0),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.0471975511965976-0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		wing1weld.C0=wing1weld.C0:Lerp(cf(-2,-1,0)*angles(-3.141592653589793,0.3490658503988659-0.5235987755982988*sin(sine*3),-3.141592653589793-0.3490658503988659*sin(sine*3)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(0.9,0.4,0)*angles(-0.8203047484373349,1.3962634015954636,1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-0.9,0.4,0)*angles(-1.0471975511965976-0.08726646259971647*sin((sine+0.05)*1),-1.3962634015954636-0.17453292519943295*sin(sine*1),-1.5707963267948966),deltaTime) 
		gniwweld.C0=gniwweld.C0:Lerp(cf(2,-1,0)*angles(-3.141592653589793,-0.3490658503988659+0.5235987755982988*sin(sine*3),-3.141592653589793+0.3490658503988659*sin(sine*3)),deltaTime) 
		end,
		walk = function()
		t.setWalkSpeed(60)
			local Vfw, Vrt = velbycfrvec()
 
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1+0.3*sin((sine + 0.1)*12),-1.5 * sin(sine*12))*angles(1.3962634015954636*sin(sine*12),-1.5707963267948966+0.17453292519943295*sin(sine*12),0),deltaTime) 
		swordweld.C0=swordweld.C0:Lerp(cf(1.5,2.8,1.8)*angles(-1.5707963267948966,0.6981317007977318,1.5707963267948966),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.1 * sin(sine*24),0)*angles(-1.7453292519943295+0.03490658503988659*sin(sine*24),0,3.141592653589793+0.08726646259971647*sin(sine*12)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1-0.3*sin((sine + 0.1)*12),1.5 * sin(sine*12))*angles(-1.3962634015954636*sin(sine*12),1.5707963267948966+0.17453292519943295*sin(sine*12),0),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.4311699866353502+0.03490658503988659*sin((sine+1)*12),0,3.141592653589793),deltaTime) 
		wing1weld.C0=wing1weld.C0:Lerp(cf(-2,-1,0)*angles(-3.141592653589793,0.3490658503988659-0.5235987755982988*sin(sine*3),-3.141592653589793-0.3490658503988659*sin(sine*3)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5,0)*angles(-1.5707963267948966,1.3962634015954636,1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.4,0.5 * sin(sine*12))*angles(1.1344640137963142*sin((sine+0.25)*12),-1.5707963267948966+0.6108652381980153*sin(sine*12),0),deltaTime) 
		gniwweld.C0=gniwweld.C0:Lerp(cf(2,-1,0)*angles(-3.141592653589793,-0.3490658503988659+0.5235987755982988*sin(sine*3),-3.141592653589793+0.3490658503988659*sin(sine*3)),deltaTime) 
		end,
		jump = function()
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-0.9-0.1*sin(sine*1),-0.6)*angles(0.6981317007977318-0.08726646259971647*sin(sine*1),-1.4835298641951802-0.08726646259971647*sin(sine*1),0),deltaTime) 
		swordweld.C0=swordweld.C0:Lerp(cf(3.3,1.2,-1.5)*angles(-0.8726646259971648,1.5707963267948966,4.014257279586958),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,-0.2+0.1*sin(sine*1),0)*angles(-2.0943951023931953+0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.7-0.1*sin(sine*1),0.2)*angles(-0.17453292519943295-0.08726646259971647*sin(sine*1),1.4835298641951802+0.08726646259971647*sin(sine*1),0),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.0471975511965976-0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		wing1weld.C0=wing1weld.C0:Lerp(cf(-2,-1,0)*angles(-3.141592653589793,0.3490658503988659-0.5235987755982988*sin(sine*3),-3.141592653589793-0.3490658503988659*sin(sine*3)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(0.9,0.4,0)*angles(-0.8203047484373349,1.3962634015954636,1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-0.9,0.4,0)*angles(-1.0471975511965976-0.08726646259971647*sin((sine+0.05)*1),-1.3962634015954636-0.17453292519943295*sin(sine*1),-1.5707963267948966),deltaTime) 
		gniwweld.C0=gniwweld.C0:Lerp(cf(2,-1,0)*angles(-3.141592653589793,-0.3490658503988659+0.5235987755982988*sin(sine*3),-3.141592653589793+0.3490658503988659*sin(sine*3)),deltaTime) 
		end,
		fall = function()
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1+0.3*sin((sine + 0.1)*12),-1.5 * sin(sine*12))*angles(1.3962634015954636*sin(sine*12),-1.5707963267948966+0.17453292519943295*sin(sine*12),0),deltaTime) 
		swordweld.C0=swordweld.C0:Lerp(cf(1.5,2.8,1.8)*angles(-1.5707963267948966,0.6981317007977318,1.5707963267948966),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.1 * sin(sine*24),0)*angles(-1.7453292519943295+0.03490658503988659*sin(sine*24),0,3.141592653589793+0.08726646259971647*sin(sine*12)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1-0.3*sin((sine + 0.1)*12),1.5 * sin(sine*12))*angles(-1.3962634015954636*sin(sine*12),1.5707963267948966+0.17453292519943295*sin(sine*12),0),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.4311699866353502+0.03490658503988659*sin((sine+1)*12),0,3.141592653589793),deltaTime) 
		wing1weld.C0=wing1weld.C0:Lerp(cf(-2,-1,0)*angles(-3.141592653589793,0.3490658503988659-0.5235987755982988*sin(sine*3),-3.141592653589793-0.3490658503988659*sin(sine*3)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5,0)*angles(-1.5707963267948966,1.3962634015954636,1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.4,0.5 * sin(sine*12))*angles(1.1344640137963142*sin((sine+0.25)*12),-1.5707963267948966+0.6108652381980153*sin(sine*12),0),deltaTime) 
		gniwweld.C0=gniwweld.C0:Lerp(cf(2,-1,0)*angles(-3.141592653589793,-0.3490658503988659+0.5235987755982988*sin(sine*3),-3.141592653589793+0.3490658503988659*sin(sine*3)),deltaTime) 
		end
	})
end)
 
btn("hammer", function()
	local t=reanimate()
	if type(t)~="table" then return end
	local raycastlegs=t.raycastlegs
	local velbycfrvec=t.velbycfrvec
	local velchgbycfrvec=t.velchgbycfrvec
	local addmode=t.addmode
	local getJoint=t.getJoint
	local getPartFromMesh=t.getPartFromMesh
	local getPartJoint=t.getPartJoint
	local RootJoint=getJoint("RootJoint")
	local RightShoulder=getJoint("Right Shoulder")
	local LeftShoulder=getJoint("Left Shoulder")
	local RightHip=getJoint("Right Hip")
	local LeftHip=getJoint("Left Hip")
	local Neck=getJoint("Neck")
	local hammerhandle = getPartFromMesh(14925980064,14925980166)
	local hammerweld = getPartJoint(hammerhandle)
	local maskhandle = getPartFromMesh(6372340487,6398568971)
	local maskweld = getPartJoint(maskhandle)
	t.setWalkSpeed(90)
 
	addmode("default", {
		idle = function()
			local rY, lY = raycastlegs()
			 if attackAnimation then return attackAnimation() end 
 
			local Cfw, Crt = velchgbycfrvec()
 
			RightHip.C0=RightHip.C0:Lerp(cf(1-0.05*sin(sine*1),-1.05-0.03*sin(sine*1),-0.2-0.1*sin(sine*1))*angles(-1.7453292519943295+0.10471975511965978*sin(sine*1),1.3962634015954636+0.05235987755982989*sin(sine*1),1.5707963267948966),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0,0)*angles(-1.4835298641951802-0.10471975511965978*sin(sine*1),0,2.6179938779914944),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.8325957145940461+0.05235987755982989*sin(sine*1),0,3.6651914291880923),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0,0)*angles(0.7853981633974483+0.08726646259971647*sin(sine*1),1.3962634015954636,1.5707963267948966),deltaTime) 
		maskweld.C0=maskweld.C0:Lerp(cf(0.2+0.05*sin(sine*1),0.1,0.5)*angles(-0.17453292519943295,0,0.17453292519943295),deltaTime) 
		hammerweld.C0=hammerweld.C0:Lerp(cf(2.1-0.03*sin(sine*1),0.5,1.7)*angles(-1.5707963267948966,-0.08726646259971647*sin(sine*1),1.6406094968746698),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1-0.05*sin(sine*1),-1.1+0.06*sin(sine*1),-0.2-0.1*sin(sine*1))*angles(1.2217304763960306+0.10471975511965978*sin(sine*1),-1.7453292519943295-0.05235987755982989*sin(sine*1),1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.2,0.6)*angles(-3.036872898470133+0.03490658503988659*sin(sine*1),-1.0471975511965976-0.03490658503988659*sin(sine*1),1.0471975511965976),deltaTime) 
		end,
		walk = function()
			local Vfw, Vrt = velbycfrvec()
 
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.4835298641951802+0.017453292519943295*sin((sine+4)*-8),0,3.141592653589793-0.08726646259971647*sin((sine-4)*8)),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.5,0)*angles(1.5707963267948966,-1.7453292519943295,1.5707963267948966+1.0471975511965976*sin(sine*8)),deltaTime) 
		hammerweld.C0=hammerweld.C0:Lerp(cf(2.1-0.03*sin(sine*1),0.5,1.7)*angles(-1.5707963267948966,-0.08726646259971647*sin(sine*1),1.6406094968746698),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.2+0.3*sin((sine - 7)*8),-0.4 * sin((sine+7)*8))*angles(1.0471975511965976*sin((sine+7)*8),-1.5707963267948966+0.17453292519943295*sin(sine*8),0),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0,0)*angles(-1.9198621771937625+0.017453292519943295*sin((sine+4)*-8),0,3.141592653589793+0.08726646259971647*sin((sine-4)*8)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.2+0.3*sin((sine - 7)*-8),0.4 * sin((sine+7)*8))*angles(-1.0471975511965976*sin((sine+7)*8),1.5707963267948966+0.17453292519943295*sin(sine*8),0),deltaTime) 
		maskweld.C0=maskweld.C0:Lerp(cf(1.7,2.3,-0.7-0.8*sin(sine*8))*angles(0.3490658503988659*sin(sine*8),0,0),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0,0)*angles(0.7853981633974483+0.08726646259971647*sin(sine*1),1.3962634015954636,1.5707963267948966),deltaTime) 
		end,
		jump = function()
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			RightHip.C0=RightHip.C0:Lerp(cf(1-0.05*sin(sine*1),-1.05-0.03*sin(sine*1),-0.2-0.1*sin(sine*1))*angles(-1.7453292519943295+0.10471975511965978*sin(sine*1),1.3962634015954636+0.05235987755982989*sin(sine*1),1.5707963267948966),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0,0)*angles(-1.4835298641951802-0.10471975511965978*sin(sine*1),0,2.6179938779914944),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.8325957145940461+0.05235987755982989*sin(sine*1),0,3.6651914291880923),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0,0)*angles(0.7853981633974483+0.08726646259971647*sin(sine*1),1.3962634015954636,1.5707963267948966),deltaTime) 
		maskweld.C0=maskweld.C0:Lerp(cf(0.2+0.05*sin(sine*1),0.1,0.5)*angles(-0.17453292519943295,0,0.17453292519943295),deltaTime) 
		hammerweld.C0=hammerweld.C0:Lerp(cf(2.1-0.03*sin(sine*1),0.5,1.7)*angles(-1.5707963267948966,-0.08726646259971647*sin(sine*1),1.6406094968746698),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1-0.05*sin(sine*1),-1.1+0.06*sin(sine*1),-0.2-0.1*sin(sine*1))*angles(1.2217304763960306+0.10471975511965978*sin(sine*1),-1.7453292519943295-0.05235987755982989*sin(sine*1),1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.2,0.6)*angles(-3.036872898470133+0.03490658503988659*sin(sine*1),-1.0471975511965976-0.03490658503988659*sin(sine*1),1.0471975511965976),deltaTime) 
		end,
		fall = function()
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.4835298641951802+0.017453292519943295*sin((sine+4)*-8),0,3.141592653589793-0.08726646259971647*sin((sine-4)*8)),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.5,0)*angles(1.5707963267948966,-1.7453292519943295,1.5707963267948966+1.0471975511965976*sin(sine*8)),deltaTime) 
		hammerweld.C0=hammerweld.C0:Lerp(cf(2.1-0.03*sin(sine*1),0.5,1.7)*angles(-1.5707963267948966,-0.08726646259971647*sin(sine*1),1.6406094968746698),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.2+0.3*sin((sine - 7)*8),-0.4 * sin((sine+7)*8))*angles(1.0471975511965976*sin((sine+7)*8),-1.5707963267948966+0.17453292519943295*sin(sine*8),0),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0,0)*angles(-1.9198621771937625+0.017453292519943295*sin((sine+4)*-8),0,3.141592653589793+0.08726646259971647*sin((sine-4)*8)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.2+0.3*sin((sine - 7)*-8),0.4 * sin((sine+7)*8))*angles(-1.0471975511965976*sin((sine+7)*8),1.5707963267948966+0.17453292519943295*sin(sine*8),0),deltaTime) 
		maskweld.C0=maskweld.C0:Lerp(cf(1.7,2.3,-0.7-0.8*sin(sine*8))*angles(0.3490658503988659*sin(sine*8),0,0),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0,0)*angles(0.7853981633974483+0.08726646259971647*sin(sine*1),1.3962634015954636,1.5707963267948966),deltaTime) 
		end
	})
end)
 
btn("Exotic glitcher", function()
	local t=reanimate()
	if type(t)~="table" then return end
	local raycastlegs=t.raycastlegs
	local velbycfrvec=t.velbycfrvec
	local velchgbycfrvec=t.velchgbycfrvec
	local addmode=t.addmode
	local getJoint=t.getJoint
	local getPartFromMesh=t.getPartFromMesh
	local getPartJoint=t.getPartJoint
	local RootJoint=getJoint("RootJoint")
	local RightShoulder=getJoint("Right Shoulder")
	local LeftShoulder=getJoint("Left Shoulder")
	local RightHip=getJoint("Right Hip")
	local LeftHip=getJoint("Left Hip")
	local Neck=getJoint("Neck")
	local wingredhandle = getPartFromMesh(4315410540,4315250791)
	local wingredweld = getPartJoint(wingredhandle)
	local winggoldhandle = getPartFromMesh(4315410540,4458626951)
	local winggoldweld = getPartJoint(winggoldhandle)
	local wingpurhandle = getPartFromMesh(4315410540,4794299274)
	local wingpurweld = getPartJoint(wingpurhandle)
	local wingbluehandle = getPartFromMesh(4315410540,4506940486)
	local wingblueweld = getPartJoint(wingbluehandle)
	t.setWalkSpeed(50)
 
	addmode("default", {
		idle = function()
		t.setWalkSpeed(50)
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			RootJoint.C0=RootJoint.C0:Lerp(cf(0,-0.2+0.1*sin(sine*1),0)*angles(-2.0943951023931953+0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		wingredweld.C0=wingredweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,17453.275066650775*sin(sine*0.001)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(0.9,0.4,0)*angles(-1.0471975511965976-0.08726646259971647*sin((sine+0.05)*1),1.3962634015954636+0.08726646259971647*sin(sine*1),1.5707963267948966),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,-3.141592653589793+17453.275066650775*sin(sine*0.001)),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,1.5707963267948966+17453.275066650775*sin(sine*0.001)),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-0.9-0.1*sin(sine*1),-0.6)*angles(0.6981317007977318-0.08726646259971647*sin(sine*1),-1.4835298641951802-0.08726646259971647*sin(sine*1),0),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-2.2689280275926285,0,3.141592653589793),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-0.9,0.4,0)*angles(-1.0471975511965976-0.08726646259971647*sin((sine+0.05)*1),-1.3962634015954636-0.08726646259971647*sin(sine*1),-1.5707963267948966),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.7-0.1*sin(sine*1),0.2)*angles(-0.17453292519943295-0.08726646259971647*sin(sine*1),1.4835298641951802+0.08726646259971647*sin(sine*1),0),deltaTime) 
		wingblueweld.C0=wingblueweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,-1.5707963267948966+17453.275066650775*sin(sine*0.001)),deltaTime) 
		end,
		walk = function()
		t.setWalkSpeed(50)
			local Vfw, Vrt = velbycfrvec()
 
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.1 * sin(sine*24),0)*angles(-1.7453292519943295+0.03490658503988659*sin(sine*24),0,3.141592653589793+0.08726646259971647*sin(sine*12)),deltaTime) 
		wingredweld.C0=wingredweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,17453.275066650775*sin(sine*0.0005)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.4,-0.5 * sin(sine*12))*angles(-1.1344640137963142*sin((sine+0.25)*12),1.5707963267948966+0.6108652381980153*sin(sine*12),0),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,-3.141592653589793+17453.275066650775*sin(sine*0.0005)),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(-4,-4,-2)*angles(0,0,1.5707963267948966+17453.275066650775*sin(sine*0.0005)),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1+0.3*sin((sine + 0.1)*12),-1.5 * sin(sine*12))*angles(1.3962634015954636*sin(sine*12),-1.5707963267948966+0.17453292519943295*sin(sine*12),0),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.4311699866353502+0.03490658503988659*sin((sine+1)*12),0,3.141592653589793),deltaTime) 
