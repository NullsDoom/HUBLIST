btn("the classic > Beta", function()
game.StarterGui:SetCore("SendNotification", {
	Title = "Dev Msg";
	Text = "this is a secret good job finding it! | this is a dev version tho so i will change things as i want and do what ever as its not made for the main hub and its just a meme";
	Icon = "http://www.roblox.com/asset/?id=16274033459";
	Duration = "140"

	})
	local t=reanimate()
	if type(t)~="table" then return end
	local addmode=t.addmode
	local refresh=t.refreshjoints
	local getJoint=t.getJoint
	local getPartFromMesh=t.getPartFromMesh
	local getPartJoint=t.getPartJoint
	local RootJoint=getJoint("RootJoint")
	local RightShoulder=getJoint("Right Shoulder")
	local LeftShoulder=getJoint("Left Shoulder")
	local RightHip=getJoint("Right Hip")
	local LeftHip=getJoint("Left Hip")
	local Neck=getJoint("Neck")
	local SwordWeld = getPartFromMesh(14774841896,14774847637)
	local SwordWeld = getPartJoint(SwordWeld)
	local BtoolsWeld = getPartFromMesh(18431821898,18431821929)
	local BtoolsWeld = getPartJoint(BtoolsWeld)
	local HammerWeld = getPartFromMesh(14463205245,14463205530)
	local HammerWeld = getPartJoint(HammerWeld)
	local CoilWeld = getPartFromMesh(18385651164,18385651291)
	local CoilWeld = getPartJoint(CoilWeld)
	local cframes=t.cframes
	local rootpart1=t.getPart("HumanoidRootPart")
	local rootpart=t.getPart("Head")
	local i1=i("TextLabel") 
	local i2=i("BillboardGui") 
	i1.Font=e.Font.Arcade
	i1.FontSize=e.FontSize.Size14 
	i1.Text="<- the classics ->" 
	i1.TextColor3=c3(0.4, 0.4, 0.4)
	i1.TextScaled=true 
	i1.TextStrokeTransparency=0 
	i1.BackgroundColor3=c3(0,0,0) 
	i1.TextStrokeColor3=c3(0,0.2,0)
	i1.BackgroundTransparency=0.5
	i1.Size=u2(1,0,0.5,0) 
	i1.Name=rs() 
	i1.Parent=i2 
	i2.AlwaysOnTop=true 
	i2.ClipsDescendants=true 
	i2.LightInfluence=1 
	i2.Size=u2(10,0,1.75,0) 
	i2.StudsOffset=v3(0,1.75,0) 
	i2.StudsOffsetWorldSpace=cframes[rootpart].Position 
	i2.ResetOnSpawn=false 
	i2.Name=rs() 
	i2.Parent=pg 

	local i1=i("TextBox") 
	i1.Font=e.Font.Arcade
	i1.FontFace=Font.new("rbxasset://fonts/families/PressStart2P.json",e.FontWeight.Bold,e.FontStyle.Normal) 
	i1.FontSize=e.FontSize.Size32 
	i1.Text="Dev Work fr fr" 
	i1.TextColor3=c3(1,1,1) 
	i1.TextSize=30 
	i1.TextStrokeTransparency=0.5
	i1.TextXAlignment=e.TextXAlignment.Left 
	i1.TextYAlignment=e.TextYAlignment.Bottom 
	i1.BackgroundColor3=c3(1,1,1) 
	i1.BackgroundTransparency=0.8 
	i1.BorderColor3=c3(0,0,0) 
	i1.BorderSizePixel=0 
	i1.Position=u2(0,0,1,-1) 
	i1.Name=rs() 
	i1.Parent=i9

	local sounds = {}

	local function ShadeID(ID, Duration, Looped)
		local s = Instance.new("Sound")
		s.SoundId = "rbxassetid://" .. ID
		if Looped then
			s.Looped = true
		end
		sounds[s]=true
		s.Parent = workspace
		s:Play()
		task.wait(Duration)
		sounds[s]=nil
		s:Destroy()
	end
	
	tspawn(function() 
		while c do
			twait()
		end 
		i2:Destroy()
		i1:Destroy() 
		ShadeID = emptyfunction
		for i,v in pairs(sounds) do
			i:Destroy()
		end
	end)


	task.spawn(ShadeID, 16190782786, 9999999, true)
	
	local Hammer = nil
	
	local attackAnimation=nil
	mouse.Button1Down:Connect(function()
		task.spawn(ShadeID, 6798698437, 2, false)
		if Hammer then 
			if attackAnimation then return end
			attackAnimation=function()
				t.setWalkSpeed(16)
				LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0,-1.5707963267948966,1.3089969389957472)),deltaTime) 
