			end
			task.wait(0.5)
			attackAnimation=function()
				t.setWalkSpeed(16)
				LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0,-1.5707963267948966,1.3089969389957472)),deltaTime) 
