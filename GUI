	i1.TextStrokeColor3=c3(0,0,0)
	i1.BackgroundTransparency=0.99
	i1.Size=u2(1,0,0.5,0) 
	i1.Name=rs() 
	i1.Parent=i2 
	i2.AlwaysOnTop=true 
