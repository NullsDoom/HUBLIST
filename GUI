		wingblueweld.C0=wingblueweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+8.5)*9),0,3.141592653589793+17453.275066650775*sin(sine*0.0002)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-0.8726646259971648+0.17453292519943295*sin((sine+0.2)*1),0.17453292519943295*sin((sine+0.9)*-1.5),3.141592653589793),deltaTime) 
		end,
		jump = function()
		t.setWalkSpeed(80)
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			wingredweld.C0=wingredweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+2.5)*9),0,17453.275066650775*sin(sine*0.0002)),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+6)*9),0,1.5707963267948966+17453.275066650775*sin(sine*0.0002)),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-0.8,0.4,-0.2)*angles(2.792526803190927+0.08726646259971647*sin((sine+0.3)*1),-0.2617993877991494+0.08726646259971647*sin((sine+0.4)*1),2.0943951023931953),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.05,0)*angles(0.8726646259971648-0.4363323129985824*sin((sine+0.8)*1),-1.6580627893946132+0.08726646259971647*sin((sine+0.3)*1.5),0.8726646259971648),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5,0.2+0.2*sin(sine*1))*angles(-2.9670597283903604,1.3962634015954636+0.17453292519943295*sin((sine+0.8)*1),1.5707963267948966),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,3+1*sin(sine*1),0)*angles(-0.17453292519943295,0.08726646259971647*sin((sine+0.3)*1.5),3.141592653589793),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+4)*9),0,-1.5707963267948966+17453.275066650775*sin(sine*0.0002)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.05,0)*angles(-1.3962634015954636-0.2617993877991494*sin((sine+0.2)*1),1.3962634015954636-0.08726646259971647*sin((sine+0.3)*1.5),0.8726646259971648),deltaTime) 
		wingblueweld.C0=wingblueweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+8.5)*9),0,3.141592653589793+17453.275066650775*sin(sine*0.0002)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.5707963267948966+0.3490658503988659*sin((sine+0.2)*1),0.17453292519943295*sin((sine+0.9)*-1.5),3.141592653589793),deltaTime)
		end,
		fall = function()
		t.setWalkSpeed(80)
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			wingredweld.C0=wingredweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+2.5)*9),0,17453.275066650775*sin(sine*0.0002)),deltaTime) 
