btn("redeemer", function()
	local t=reanimate()
	if type(t)~="table" then return end
	local raycastlegs=t.raycastlegs
	local velbycfrvec=t.velbycfrvec
	local velchgbycfrvec=t.velchgbycfrvec
	local addmode=t.addmode
	local getJoint=t.getJoint
	local getPartFromMesh=t.getPartFromMesh
	local getPartJoint=t.getPartJoint
	local RootJoint=getJoint("RootJoint")
	local RightShoulder=getJoint("Right Shoulder")
	local LeftShoulder=getJoint("Left Shoulder")
	local RightHip=getJoint("Right Hip")
	local LeftHip=getJoint("Left Hip")
	local Neck=getJoint("Neck")
	local swordhandle = getPartFromMesh(6050886271,6077105291)
	local swordweld = getPartJoint(swordhandle)
	local bunnyhandle = getPartFromMesh(4846239829,4846223738)
	local bunnyweld = getPartJoint(bunnyhandle)
	t.setWalkSpeed(15)
 
	addmode("default", {
		idle = function()
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5+0.05*sin(sine*1),0)*angles(0.6981317007977318-0.08726646259971647*sin(sine*1),1.7453292519943295+0.08726646259971647*sin(sine*1),-0.8726646259971648),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.05 * sin(sine*1),0)*angles(-1.4835298641951802+0.04363323129985824*sin(sine*1),0,3.141592653589793),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.7453292519943295-0.04363323129985824*sin(sine*1),0,3.141592653589793+0.08726646259971647*sin(sine*1)),deltaTime) 
		bunnyweld.C0=bunnyweld.C0:Lerp(cf(0,-0.65,0.3)*angles(0.05235987755982989*sin(sine*1),0.05235987755982989*sin(sine*1),0.17453292519943295+0.05235987755982989*sin(sine*1)),deltaTime) 
		swordweld.C0=swordweld.C0:Lerp(cf(-1.2,0,1)*angles(-1.5707963267948966,-0.3490658503988659,1.5707963267948966),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.1-0.05*sin(sine*1),0)*angles(1.1344640137963142-0.09599310885968812*sin(sine*1),1.7453292519943295,-1.3962634015954636),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.1-0.05*sin(sine*1),0)*angles(-1.5707963267948966-0.09599310885968812*sin(sine*1),-1.4835298641951802,-1.5707963267948966),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.45+0.05*sin(sine*1),0)*angles(-1.9198621771937625+0.08726646259971647*sin(sine*1),-2.0943951023931953+0.08726646259971647*sin(sine*1),-1.5707963267948966),deltaTime) 
			--LeftArm,-1,0,0,1,15,15,0,1,0.3,-0.1,0,1,-85,5,0,1,0,0,0,1,0,0,0,1,RobloxVisor2019_Handle,8.658389560878277e-09,0,0,1,0,0,0,1,0.19000005722045898,0,0,1,0,0,0,1,0.17972776293754578,0,0,1,0,0,0,1,Meshes/swoooordAccessory_Handle,0,0,0,1,0,0,0,1,-0.9,0.1,0,1,90,0,0,1,0.,0,0,1,170,0,0,1,LeftLeg,-1,0,0,1,-10,5,0,1,-1,0,0,1,-80,5,0,1,0,0,0,1,0,0,0,1,PlaneModel_Handle,0,0,0,1,-10,-5,0,1,2,0,0,1,0,0,0,1,0,0,0,1,-0,0,0,1,RightLeg,1,0,0,1,20,5,0,1,-1,0,0,1,80,5,0,1,0,0,0,1,0,0,0,1,Head,0,0,0,1,-90,-10,0,1,1,0,0,1,-0,0,0,1,0,0,0,1,180,0,0,1,Torso,0,0,0,1,-100,5,0,1,3,1,0,1,-0,0,0,1,0,1,2,1,180,0,0,1,RightArm,1,0,0,1,15,-15,0,1,0.3,0,0,1,85,5,0,1,0,0,0,1,0,0,0,1
		end,
		walk = function()
			local Vfw, Vrt = velbycfrvec()
 
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
			t.setWalkSpeed(15)
 
			RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5+0.05*sin(sine*6),0)*angles(0.17453292519943295+1.0471975511965976*sin((sine+0.2)*6),1.5707963267948966+0.2617993877991494*sin((sine+0.2)*6),0),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.05 * sin((sine+5)*12),0)*angles(-1.9198621771937625,0,3.141592653589793+0.08726646259971647*sin(sine*6)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.2217304763960306-0.017453292519943295*sin((sine+4)*5),0,3.141592653589793+0.08726646259971647*sin((sine+4)*5)),deltaTime) 
		bunnyweld.C0=bunnyweld.C0:Lerp(cf(0,-0.65,0.3)*angles(0.04363323129985824*sin(sine*5),-0.08726646259971647*sin(sine*5),0.17453292519943295+0.10471975511965978*sin(sine*5)),deltaTime) 
		swordweld.C0=swordweld.C0:Lerp(cf(-1.1+0.05*sin(sine*5),0,1)*angles(-1.5707963267948966,-0.3490658503988659+0.04363323129985824*sin(sine*5),1.5707963267948966),deltaTime) 
