			local fw,rt=velbycfrvec()
 
			Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.2217304763960306-0.13962634015954636*sin((sine+0.5)*1),0,3.2288591161895095),deltaTime) 
