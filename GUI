            AccessoryWeld.C0=Lerp(AccessoryWeld.C0,cfMul(cf(0.3,0.5,0),angles(1.5707963267948966,-0.6981317007977318,1.3962634015954636)),deltaTime) 
            RootJoint.C0=Lerp(RootJoint.C0,cfMul(cf(0.2,-0.45+0.1*sin(sine*20),0),angles(-0.6981317007977318+0.08726646259971647*sin(sine*10),0.17453292519943295+0.08726646259971647*sin(sine*1),3.141592653589793)),deltaTime) 
            LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-0.5,-1.5,0),angles(1.3962634015954636,0.5235987755982988,-0.08726646259971647)),deltaTime) 
            LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1.5,0,-0.5),angles(0.17453292519943295*sin(sine*10),-0.4363323129985824-0.17453292519943295*sin(sine*10),-0.6981317007977318)),deltaTime) 
            Neck.C0=Lerp(Neck.C0,cfMul(cf(0,1,0),angles(-2.0943951023931953+0.08726646259971647*sin(sine*10),-0.17453292519943295+0.17453292519943295*sin(sine*5),3.141592653589793)),deltaTime) 
