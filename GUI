		wingblueweld.C0=wingblueweld.C0:Lerp(cf(5+2*sin(sine*2),5+2*sin(sine*2),-1.5)*angles(0.3490658503988659*sin((sine+8.5)*9),0,3.141592653589793+17453.275066650775*sin(sine*0.0002)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-0.8726646259971648+0.17453292519943295*sin((sine+0.2)*1),0.17453292519943295*sin((sine+0.9)*-1.5),3.141592653589793),deltaTime) 
		end
	})
 
	addmode("r", {
		idle = function()
		t.setWalkSpeed(10)
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.3,-0.2+0.1*sin(sine*1))*angles(-0.17453292519943295-0.08726646259971647*sin(sine*1),-1.5707963267948966,0),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5,0)*angles(-0.3490658503988659-0.08726646259971647*sin(sine*1),1.3962634015954636,0),deltaTime) 
		wingredweld.C0=wingredweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),2.356194490192345+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1+0.05*sin(sine*1),0.5+0.1*sin(sine*1))*angles(-0.6981317007977318-0.08726646259971647*sin(sine*1),1.5707963267948966,0),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),0.17453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0,0)*angles(-1.2217304763960306+0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),-1.7453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.9198621771937625-0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.5,0)*angles(-0.3490658503988659-0.08726646259971647*sin(sine*1),-1.3962634015954636,0),deltaTime) 
		wingblueweld.C0=wingblueweld.C0:Lerp(cf(0,0,-2+1*sin(sine*1))*angles(0,0,-17453.275066650775*sin(sine*0.002)),deltaTime) 
		end,
		walk = function()
		t.setWalkSpeed(10)
			local Vfw, Vrt = velbycfrvec()
 
			local rY, lY = raycastlegs()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.1+0.3*sin((sine + 0.2)*6),-0.7 * sin(sine*6))*angles(0.6981317007977318*sin((sine+0.1)*6),-1.5707963267948966+0.17453292519943295*sin((sine+0.2)*6),0),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.4,-0.4 * sin(sine*6))*angles(-0.6108652381980153*sin((sine+1.5)*6),1.5707963267948966+0.2617993877991494*sin(sine*6),0),deltaTime) 
		wingredweld.C0=wingredweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),2.356194490192345+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.1-0.3*sin((sine + 0.2)*6),0.7 * sin(sine*6))*angles(-0.6981317007977318*sin((sine+0.1)*6),1.5707963267948966+0.17453292519943295*sin((sine+0.2)*6),0),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),0.17453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.1 * sin(sine*12),0)*angles(-1.7453292519943295+0.03490658503988659*sin(sine*12),0,3.141592653589793+0.08726646259971647*sin(sine*6)),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),-1.7453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.3962634015954636-0.03490658503988659*sin((sine+2)*12),0,3.141592653589793-0.08726646259971647*sin(sine*6)),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.4,0.4 * sin(sine*6))*angles(0.6108652381980153*sin((sine+1.5)*6),-1.5707963267948966+0.2617993877991494*sin(sine*6),0),deltaTime) 
		wingblueweld.C0=wingblueweld.C0:Lerp(cf(0,0,-2+1*sin(sine*1))*angles(0,0,-17453.275066650775*sin(sine*0.002)),deltaTime) 
		end,
		jump = function()
		t.setWalkSpeed(10)
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.3,-0.2+0.1*sin(sine*1))*angles(-0.17453292519943295-0.08726646259971647*sin(sine*1),-1.5707963267948966,0),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5,0)*angles(-0.3490658503988659-0.08726646259971647*sin(sine*1),1.3962634015954636,0),deltaTime) 
		wingredweld.C0=wingredweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),2.356194490192345+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1+0.05*sin(sine*1),0.5+0.1*sin(sine*1))*angles(-0.6981317007977318-0.08726646259971647*sin(sine*1),1.5707963267948966,0),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),0.17453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0,0)*angles(-1.2217304763960306+0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),-1.7453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.9198621771937625-0.08726646259971647*sin(sine*1),0,3.141592653589793),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.5,0)*angles(-0.3490658503988659-0.08726646259971647*sin(sine*1),-1.3962634015954636,0),deltaTime) 
		wingblueweld.C0=wingblueweld.C0:Lerp(cf(0,0,-2+1*sin(sine*1))*angles(0,0,-17453.275066650775*sin(sine*0.002)),deltaTime) 
		end,
		fall = function()
		t.setWalkSpeed(10)
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.1+0.3*sin((sine + 0.2)*6),-0.7 * sin(sine*6))*angles(0.6981317007977318*sin((sine+0.1)*6),-1.5707963267948966+0.17453292519943295*sin((sine+0.2)*6),0),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.4,-0.4 * sin(sine*6))*angles(-0.6108652381980153*sin((sine+1.5)*6),1.5707963267948966+0.2617993877991494*sin(sine*6),0),deltaTime) 
		wingredweld.C0=wingredweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),2.356194490192345+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1.1-0.3*sin((sine + 0.2)*6),0.7 * sin(sine*6))*angles(-0.6981317007977318*sin((sine+0.1)*6),1.5707963267948966+0.17453292519943295*sin((sine+0.2)*6),0),deltaTime) 
		winggoldweld.C0=winggoldweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),0.17453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.1 * sin(sine*12),0)*angles(-1.7453292519943295+0.03490658503988659*sin(sine*12),0,3.141592653589793+0.08726646259971647*sin(sine*6)),deltaTime) 
		wingpurweld.C0=wingpurweld.C0:Lerp(cf(5+1*sin(sine*1),5+1*sin(sine*1),0)*angles(0,-0.6981317007977318+0.08726646259971647*sin(sine*1),-1.7453292519943295+17453.275066650775*sin(sine*0.001)),deltaTime) 
		Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.3962634015954636-0.03490658503988659*sin((sine+2)*12),0,3.141592653589793-0.08726646259971647*sin(sine*6)),deltaTime) 
		LeftShoulder.C0=LeftShoulder.C0:Lerp(cf(-1,0.4,0.4 * sin(sine*6))*angles(0.6108652381980153*sin((sine+1.5)*6),-1.5707963267948966+0.2617993877991494*sin(sine*6),0),deltaTime) 
