				RightHip.C0=Lerp(RightHip.C0,cfMul(cf(1,-1,0),angles(0,1.5707963267948966,0.4363323129985824)),deltaTime) 
				BtoolsWeld.C0=Lerp(BtoolsWeld.C0,cf(0,1,0),deltaTime) 
				HammerWeld.C0=Lerp(HammerWeld.C0,cfMul(cf(3.5,-2.5,-1.5),angles(1.5707963267948966,0.8726646259971648,1.5707963267948966)),deltaTime)
				CoilWeld.C0=Lerp(CoilWeld.C0,cf(0,0,-15),deltaTime) 
				SwordWeld.C0=Lerp(SwordWeld.C0,cfMul(cf(-0.6,1.1,-0.55),angles(0,0,-2.530727415391778)),deltaTime) 
				i2.StudsOffsetWorldSpace=cframes[rootpart].Position 
			end
			task.wait(0.2)
			attackAnimation=nil		
		else
			if attackAnimation then return end
			attackAnimation=function()
				t.setWalkSpeed(16)
				LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0,-1.5707963267948966,1.3089969389957472)),deltaTime) 
				LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1,0.5,0),angles(0,-1.5707963267948966,0)),deltaTime) 
				Neck.C0=Lerp(Neck.C0,cfMul(cf(0,1,0),angles(-1.5707963267948966,0,3.141592653589793)),deltaTime) 
