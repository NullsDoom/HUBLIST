		end,
		jump = function()
			local Vfw, Vrt = velbycfrvec()
 
			local Cfw, Crt = velchgbycfrvec()
 
			Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.5707963267948966+0.3490658503988659*sin(sine*5),0,3.141592653589793),deltaTime) 
