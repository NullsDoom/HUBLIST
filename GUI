		RootJoint.C0=RootJoint.C0:Lerp(cf(0,0.1,0.1 * sin(sine*1))*angles(-1.7453292519943295+0.08726646259971647*sin(sine*1),0,3.0543261909900767),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(0.7,0.5,0)*angles(-2.2689280275926285+0.04363323129985824*sin((sine+0.5)*1),2.0943951023931953+0.04363323129985824*sin(sine*1),1.5707963267948966),deltaTime) 
		LeftHip.C0=LeftHip.C0:Lerp(cf(-1,-1.1,-0.5)*angles(0.08726646259971647-0.08726646259971647*sin(sine*1),-1.3962634015954636-0.04363323129985824*sin(sine*1),0),deltaTime) 
		end,
		walk=function()
		t.setWalkSpeed(20)
			local fw,rt=velbycfrvec()
 
			Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.3962634015954636+0.04363323129985824*sin(sine*-12),0,3.141592653589793-0.08726646259971647*sin(sine*6)),deltaTime) 
		RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.4,0)*angles(-2.0943951023931953,2.0943951023931953,1.5707963267948966),deltaTime) 
		RightHip.C0=RightHip.C0:Lerp(cf(1,-1-0.3*sin((sine + 0.2)*6),0.7 * sin(sine*6))*angles(-0.6981317007977318*sin(sine*6),1.5707963267948966+0.17453292519943295*sin(sine*6),0),deltaTime) 
