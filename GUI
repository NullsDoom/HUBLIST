	addmode("default", {
		modeEntered=function()
			Hammer = false
		end,
		idle = function()
			t.setWalkSpeed(16)
				t.setJumpPower(50)
				t.setGravity(200)
			if attackAnimation then return attackAnimation() end
			LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0.1308996938995747*sin(sine*0.7),-1.5707963267948966,0)),deltaTime) 
			LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1,0.5,0),angles(-0.1308996938995747*sin(sine*0.7),-1.5707963267948966,0)),deltaTime) 
			RightHip.C0=Lerp(RightHip.C0,cfMul(cf(1,-1,0),angles(-0.1308996938995747*sin(sine*0.7),1.5707963267948966,0)),deltaTime) 
			RootJoint.C0=Lerp(RootJoint.C0,angles(-1.5707963267948966,0,3.141592653589793),deltaTime) 
			Neck.C0=Lerp(Neck.C0,cfMul(cf(0,1,0),angles(-1.5707963267948966,0,3.141592653589793)),deltaTime) 
			RightShoulder.C0=Lerp(RightShoulder.C0,cfMul(cf(1,0.5,0),angles(0.1308996938995747*sin(sine*0.7),1.5707963267948966,0)),deltaTime) 
			SwordWeld.C0=Lerp(SwordWeld.C0,cfMul(cf(-0.6,1.1,-0.55),angles(0,0,-2.530727415391778)),deltaTime) 
			BtoolsWeld.C0=Lerp(BtoolsWeld.C0,cf(0,1,0),deltaTime) 
			HammerWeld.C0=Lerp(HammerWeld.C0,cfMul(cf(1,0,-0.15),angles(-0.08726646259971647,-0.08726646259971647,-0.4363323129985824)),deltaTime) 
			CoilWeld.C0=Lerp(CoilWeld.C0,cf(0,50,0),deltaTime) 
			CoilWeld2.C0=Lerp(CoilWeld2.C0,cf(0,50,0),deltaTime)
			NoobWeld.C0=Lerp(NoobWeld.C0,cf(0,50,0),deltaTime)
			i2.StudsOffsetWorldSpace=cframes[rootpart].Position 

		end,
		walk = function()
			t.setWalkSpeed(16)
			if attackAnimation then return attackAnimation() end
			LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0.8726646259971648*sin(sine*-7),-1.5707963267948966,0)),deltaTime) 
			LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1,0.5,0),angles(0.8726646259971648*sin(sine*7),-1.5707963267948966,0)),deltaTime)  
			RightHip.C0=Lerp(RightHip.C0,cfMul(cf(1,-1,0),angles(0.8726646259971648*sin(sine*7),1.5707963267948966,0)),deltaTime) 
			RootJoint.C0=Lerp(RootJoint.C0,angles(-1.5707963267948966,0,3.141592653589793),deltaTime) 
			Neck.C0=Lerp(Neck.C0,cfMul(cf(0,1,0),angles(-1.5707963267948966,0,3.141592653589793)),deltaTime) 
			RightShoulder.C0=Lerp(RightShoulder.C0,cfMul(cf(1,0.5,0),angles(-0.8726646259971648*sin(sine*7),1.5707963267948966,0)),deltaTime) 
			SwordWeld.C0=Lerp(SwordWeld.C0,cfMul(cf(-0.6,1.1,-0.55),angles(0,0,-2.530727415391778)),deltaTime) 
			BtoolsWeld.C0=Lerp(BtoolsWeld.C0,cf(0,1,0),deltaTime) 
			HammerWeld.C0=Lerp(HammerWeld.C0,cfMul(cf(1,0,-0.15),angles(-0.08726646259971647,-0.08726646259971647,-0.4363323129985824)),deltaTime) 
			CoilWeld.C0=Lerp(CoilWeld.C0,cf(0,50,0),deltaTime) 
			CoilWeld2.C0=Lerp(CoilWeld2.C0,cf(0,50,0),deltaTime)
			NoobWeld.C0=Lerp(NoobWeld.C0,cf(0,50,0),deltaTime)
			i2.StudsOffsetWorldSpace=cframes[rootpart].Position 
		end,
		jump = function()
			t.setWalkSpeed(16)
			if attackAnimation then return attackAnimation() end
			RootJoint.C0=Lerp(RootJoint.C0,angles(-1.5707963267948966,0,3.141592653589793),deltaTime) 
			RightHip.C0=Lerp(RightHip.C0,cfMul(cf(1,-1,0),angles(0,1.5707963267948966,0)),deltaTime) 
			LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0,-1.5707963267948966,0)),deltaTime) 
			LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1,0.5,0),angles(3.141592653589793,-1.5707963267948966,0)),deltaTime) 
			Neck.C0=Lerp(Neck.C0,cfMul(cf(0,1,0),angles(-1.5707963267948966,0,3.141592653589793)),deltaTime) 
			RightShoulder.C0=Lerp(RightShoulder.C0,cfMul(cf(1,0.5,0),angles(0,1.5707963267948966,3.141592653589793)),deltaTime) 
			SwordWeld.C0=Lerp(SwordWeld.C0,cfMul(cf(-0.6,1.1,-0.55),angles(0,0,-2.530727415391778)),deltaTime) 
			BtoolsWeld.C0=Lerp(BtoolsWeld.C0,cf(0,1,0),deltaTime) 
			HammerWeld.C0=Lerp(HammerWeld.C0,cfMul(cf(1,0,-0.15),angles(-0.08726646259971647,-0.08726646259971647,-0.4363323129985824)),deltaTime) 
			CoilWeld.C0=Lerp(CoilWeld.C0,cf(150,350,0),deltaTime) 
			CoilWeld2.C0=Lerp(CoilWeld2.C0,cf(150,350,0),deltaTime)
			NoobWeld.C0=Lerp(NoobWeld.C0,cf(0,-50,0),deltaTime)
			i2.StudsOffsetWorldSpace=cframes[rootpart].Position 
		end,
		fall = function()
			t.setWalkSpeed(16)
			if attackAnimation then return attackAnimation() end
			RootJoint.C0=Lerp(RootJoint.C0,angles(-1.5707963267948966,0,3.141592653589793),deltaTime) 
			RightHip.C0=Lerp(RightHip.C0,cfMul(cf(1,-1,0),angles(0,1.5707963267948966,0)),deltaTime) 
			LeftHip.C0=Lerp(LeftHip.C0,cfMul(cf(-1,-1,0),angles(0,-1.5707963267948966,0)),deltaTime) 
			LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1,0.5,0),angles(3.141592653589793,-1.5707963267948966,0)),deltaTime) 
			Neck.C0=Lerp(Neck.C0,cfMul(cf(0,1,0),angles(-1.5707963267948966,0,3.141592653589793)),deltaTime) 
			RightShoulder.C0=Lerp(RightShoulder.C0,cfMul(cf(1,0.5,0),angles(0,1.5707963267948966,3.141592653589793)),deltaTime) 
			SwordWeld.C0=Lerp(SwordWeld.C0,cfMul(cf(-0.6,1.1,-0.55),angles(0,0,-2.530727415391778)),deltaTime) 
			BtoolsWeld.C0=Lerp(BtoolsWeld.C0,cf(0,1,0),deltaTime) 
			HammerWeld.C0=Lerp(HammerWeld.C0,cfMul(cf(1,0,-0.15),angles(-0.08726646259971647,-0.08726646259971647,-0.4363323129985824)),deltaTime) 
			CoilWeld.C0=Lerp(CoilWeld.C0,cf(150,350,0),deltaTime)
			CoilWeld2.C0=Lerp(CoilWeld2.C0,cf(150,350,0),deltaTime) 
			i2.StudsOffsetWorldSpace=cframes[rootpart].Position 
		end
	})
