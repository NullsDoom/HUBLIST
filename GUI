	Neck.C0=Neck.C0:Lerp(cf(0,1,0)*angles(-1.5707963267948966+0.3490658503988659*sin(sine*100),-0.5235987755982988+0.3490658503988659*sin(sine*100),3.141592653589793+0.3490658503988659*sin(sine*100)),deltaTime) 
