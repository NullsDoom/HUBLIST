		if attackAnimation then return end
		task.spawn(ShadeID, 200633196, 5)
		attackAnimation=function()
			RightShoulder.C0=RightShoulder.C0:Lerp(cf(1,0.5,0)*angles(0,2.356194490192345,1.5707963267948966),deltaTime) 
